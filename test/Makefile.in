PYTHON_BIN=@PYTHON_BIN@

test:
	$(PYTHON_BIN) test.py

clean:
	rm -f *.pyc *.pyo 
	cd conf && rm -f test.conf
	cd htdocs && rm -f *pyc *pyo
	rm -rf logs
	rm -rf tmp

distclean: clean
	rm -f Makefile testconf.py 
